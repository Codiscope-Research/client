{
  "protocol": "secretUi",
  "types": [
    {
      "type": "record",
      "name": "Feature",
      "fields": [
        {
          "type": "boolean",
          "name": "allow"
        },
        {
          "type": "boolean",
          "name": "defaultValue"
        },
        {
          "type": "boolean",
          "name": "readonly"
        },
        {
          "type": "string",
          "name": "label"
        }
      ]
    },
    {
      "type": "record",
      "name": "GUIEntryFeatures",
      "fields": [
        {
          "type": "Feature",
          "name": "storeSecret"
        },
        {
          "type": "Feature",
          "name": "showTyping"
        }
      ]
    },
    {
      "type": "enum",
      "name": "PassphraseType",
      "symbols": [
        "NONE_0",
        "PAPER_KEY_1",
        "PASS_PHRASE_2",
        "VERIFY_PASS_PHRASE_3"
      ]
    },
    {
      "type": "record",
      "name": "GUIEntryArg",
      "fields": [
        {
          "type": "string",
          "name": "windowTitle"
        },
        {
          "type": "string",
          "name": "prompt"
        },
        {
          "type": "string",
          "name": "username"
        },
        {
          "type": "string",
          "name": "submitLabel"
        },
        {
          "type": "string",
          "name": "cancelLabel"
        },
        {
          "type": "string",
          "name": "retryLabel"
        },
        {
          "type": "PassphraseType",
          "name": "type"
        },
        {
          "type": "GUIEntryFeatures",
          "name": "features"
        }
      ]
    },
    {
      "type": "record",
      "name": "GetPassphraseRes",
      "fields": [
        {
          "type": "string",
          "name": "passphrase"
        },
        {
          "type": "boolean",
          "name": "storeSecret"
        }
      ]
    },
    {
      "type": "record",
      "name": "SecretEntryArg",
      "fields": [
        {
          "type": "string",
          "name": "desc"
        },
        {
          "type": "string",
          "name": "prompt"
        },
        {
          "type": "string",
          "name": "err"
        },
        {
          "type": "string",
          "name": "cancel"
        },
        {
          "type": "string",
          "name": "ok"
        },
        {
          "type": "string",
          "name": "reason"
        },
        {
          "type": "boolean",
          "name": "useSecretStore"
        }
      ]
    },
    {
      "type": "record",
      "name": "SecretEntryRes",
      "fields": [
        {
          "type": "string",
          "name": "text"
        },
        {
          "type": "boolean",
          "name": "canceled"
        },
        {
          "type": "boolean",
          "name": "storeSecret"
        }
      ]
    }
  ],
  "messages": {
    "getPassphrase": {
      "request": [
        {
          "name": "sessionID",
          "type": "int"
        },
        {
          "name": "pinentry",
          "type": "GUIEntryArg"
        },
        {
          "name": "terminal",
          "type": [
            "null",
            "SecretEntryArg"
          ]
        }
      ],
      "response": "GetPassphraseRes"
    }
  },
  "namespace": "keybase.1"
}